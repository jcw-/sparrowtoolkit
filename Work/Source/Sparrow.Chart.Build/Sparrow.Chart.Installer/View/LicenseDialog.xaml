<UserControl x:Class="Sparrow.Chart.Installer.LicenseDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <FlowDocumentScrollViewer IsToolBarVisible="False" HorizontalAlignment="Center" VerticalAlignment="Top">
            <FlowDocument>
                <!--<Paragraph TextAlignment="Center" FontSize="26">
                    <Bold>
                        Readme
                    </Bold>
                </Paragraph>-->
                <Paragraph FontFamily="Times New Roman">
                    <RichTextBox IsReadOnly="True" IsTabStop="False" x:Name="licenseText"/>
                </Paragraph>
            </FlowDocument>
        </FlowDocumentScrollViewer>
        
    </Grid>
</UserControl>
